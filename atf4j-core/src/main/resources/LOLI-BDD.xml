<?xml version="1.0" encoding="UTF-8"?>
<tns:story xmlns:tns="http://www.example.org/BDD-Schema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/BDD-Schema BDD-Schema.xsd ">
	<narrative>
		<narrative>Generic Feed Message Injection</narrative>
		<role>automation test team</role>
		<value>to test central feed</value>
		<want>to inject messages into an ActiveMQ</want>
	</narrative>
	<behaviour>
		<description>Inject a Simple Message</description>
		<given>
			<description>an xml message</description>
			<detail>/GroupTwo/Simple.xml</detail>
		</given>
		<when>
			<description>When the xml message is injected into </description>
			<detail>test-queue</detail>
		</when>
		<then>
			<description>Then the queue contains the message</description>
			<detail>test-queue</detail>
		</then>
	</behaviour>
</tns:story>
