<?xml version="1.0" encoding="UTF-8"?>
<tns:story xmlns:tns="http://www.example.org/BDD-Schema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.example.org/BDD-Schema BDD-Schema.xsd ">
	<narrative>
		<Narrative-Text>Generic Feed Message Injection</Narrative-Text>
		<as-a-role>automation test team</as-a-role>
		<in-order-to>to test central feed</in-order-to>
		<I-want>to inject messages into an ActiveMQ</I-want>
	</narrative>
	<behaviour>
		<description>Inject a Simple Message</description>
		<given>
			<description>an xml message</description>
			<detail>./src/test/resources/GroupTwo/Simple.xml</detail>
		</given>
		<when>
			<description>When the xml message is injected into </description>
			<detail>loki-test-queue</detail>
		</when>
		<then>
			<description>Then the queue contains the message</description>
			<detail>loki-test-queue</detail>
		</then>
	</behaviour>
</tns:story>
